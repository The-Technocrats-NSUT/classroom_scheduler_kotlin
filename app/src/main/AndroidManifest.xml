<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.classroomScheduler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ClassroomScheduler">
        <activity android:name=".Utils.PostUpcomingEvent"></activity>
        <activity android:name=".JoinHubActivity" />
        <activity android:name=".CreateNewHubActivity" />
        <activity android:name=".Utils.PostNewNotice" />
        <activity android:name=".CurrentHubActivity" />
        <activity android:name=".HubListMainActivity" />
        <activity android:name=".SignInActivity" />
        <activity
            android:name=".IntroductionActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>